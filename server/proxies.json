{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "ProxyFirebaseAuthRoot": {
      "matchCondition": {
        "methods": ["GET", "POST"],
        "route": "/__/auth"
      },
      "backendUri": "https://roguelike-celebration-dev.firebaseapp.com/__/auth"
    },
    "ProxyFirebaseAuthRest": {
      "matchCondition": {
        "methods": ["GET", "POST"],
        "route": "/__/auth/{authPath}"
      },
      "backendUri": "https://roguelike-celebration-dev.firebaseapp.com/__/auth/{authPath}"
    }
  }
}
